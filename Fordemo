package com.examples;

public class Fordemo {

	public static void main(String args[]) {
		
		for(int i=1; i<=10;i++) {
			  System.out.println("Value of x = " + i);
		}
		
		int a = 10;
		while(a<20) {
			System.out.println("Value of a = " + a);
			a++;
		}
		
		do {
			System.out.println("Value of x = " + a);
			a++;
			
		}while(a<20); 
	}
}
